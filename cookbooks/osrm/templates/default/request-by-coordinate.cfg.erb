[modes]
modes: ["car", "bike", "foot"]

[car]
servers: ["w"]
url_w: "http://localhost:<%= @port %>/route/v1/driving/"
polygon_w: None

[bike]
servers: ["am", "eu"]
url_eu: "http://localhost:<%= @port + 1 %>/route/v1/driving/"
url_am: "http://localhost:<%= @port + 1 %>/route/v1/driving/"
polygon_am: None
polygon_eu: <%= @polyeu %>

[foot]
servers: ["am", "eu"]
url_eu: "http://localhost:<%= @port + 2 %>/route/v1/driving/"
url_am: "http://localhost:<%= @port + 3 %>/route/v1/driving/"
polygon_am: None
polygon_eu: <%= @polyeu %>
